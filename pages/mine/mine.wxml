<!--mine.wxml-->
<view class="mine">
  <view class="user_info">
    <image src='../../images/u419.jpg'></image>
    <view class='avatar'>
      <open-data type="userAvatarUrl"></open-data>
    </view>
    <view class='user'>
      <open-data type='userNickName'></open-data>
    </view>
  </view>
  <view class='contList'>
    <view catchtap='myOrder' class="weui-cells weui-cells_after-title">
      <view class="weui-cell weui-cell_access">
        <view class="weui-cell__bd">
          <image src='../../images/u425.png'></image>
          <text>我的订单</text>
        </view>
        <view class="weui-cell__ft weui-cell__ft_in-access" style="font-size: 0"></view>
      </view>
    </view>
    <view catchtap='buyer_addr' class="weui-cells weui-cells_after-title">
      <view class="weui-cell weui-cell_access">
        <view class="weui-cell__bd">
          <image src='../../images/u431.png'></image>
          <text>收货地址</text>
        </view>
        <view class="weui-cell__ft weui-cell__ft_in-access" style="font-size: 0"></view>
      </view>
      <view catchtap='contact' class="weui-cells weui-cells_after-title">
        <view class="weui-cell weui-cell_access">
          <view class="weui-cell__bd">
            <image src='../../images/u435.png'></image>
            <text>联系客服</text>
          </view>
          <view class="weui-cell__ft weui-cell__ft_in-access" style="font-size: 0"></view>
        </view>
      </view>
    </view>
  </view>
</view>